digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	2
	node [shape=circle]
	1 -> 2 [label=b]
	1 -> 3 [label=a]
	1 -> 4 [label=3]
	4 -> 5 [label=3]
	5 -> 5 [label=3]
	3 -> 6 [label=b]
	3 -> 7 [label=2]
	3 -> 8 [label=a]
	8 -> 6 [label=b]
	8 -> 7 [label=2]
	8 -> 8 [label=a]
	7 -> 6 [label=b]
	7 -> 7 [label=2]
	7 -> 8 [label=a]
	6 -> 6 [label=b]
	6 -> 7 [label=2]
	6 -> 8 [label=a]
	2 -> 6 [label=b]
	2 -> 7 [label=2]
	2 -> 8 [label=a]
}
