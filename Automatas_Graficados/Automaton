	rankdir=LR size="8,5"
	node [shape=doublecircle]
	3
	node [shape=circle]
	1 -> 2 [label=b]
	1 -> 3 [label=2]
	1 -> 4 [label=1]
	4 -> 5 [label=1]
	5 -> 5 [label=1]
	3 -> 6 [label=2]
	6 -> 7 [label=2]
	7 -> 7 [label=2]
	2 -> 8 [label=l]
	8 -> 9 [label=a]
	9 -> 10 [label=n]
	10 -> 11 [label=c]
	11 -> 12 [label=o]
	12 -> 13 [label=b]
	13 -> 14 [label=l]
	14 -> 15 [label=a]
	15 -> 16 [label=n]
	16 -> 17 [label=c]
	17 -> 18 [label=o]
	18 -> 13 [label=b]
}
