digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	2
	node [shape=circle]
	1 -> 2 [label=e]
	1 -> 3 [label=b]
	1 -> 4 [label=a]
	4 -> False [label="e|b|a"]
	3 -> False [label="e|b|a"]
	2 -> 5 [label=e]
	2 -> 6 [label=b]
	2 -> 7 [label=a]
	7 -> False [label="e|b|a"]
	6 -> False [label="e|b|a"]
	5 -> False [label="e|b|a"]
}
