digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	9
	node [shape=circle]
	1 -> 2 [label=a]
	3 -> 4 [label=b]
	2 -> 3 [label=E]
	2 -> 5 [label=E]
	4 -> 3 [label=E]
	4 -> 5 [label=E]
	5 -> 6 [label=a]
	7 -> 8 [label=b]
	6 -> 7 [label=E]
	6 -> 9 [label=E]
	8 -> 7 [label=E]
	8 -> 9 [label=E]
}
