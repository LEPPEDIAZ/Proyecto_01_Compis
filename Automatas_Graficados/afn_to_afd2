digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	5
	node [shape=circle]
	1 -> 2 [label=a]
	1 -> 3 [label=b]
	3 -> 2 [label=a]
	3 -> 3 [label=b]
	2 -> 4 [label=b]
	4 -> 5 [label=b]
	5 -> 6 [label=a]
	5 -> 7 [label=b]
	7 -> 6 [label=a]
	7 -> 7 [label=b]
	6 -> 6 [label=a]
	6 -> 7 [label=b]
}
