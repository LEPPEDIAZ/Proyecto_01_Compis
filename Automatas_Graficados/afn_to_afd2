digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	1
	node [shape=circle]
	1 -> 2 [label=0]
	1 -> 3 [label=e]
	3 -> 2 [label=0]
	3 -> 3 [label=e]
	3 -> 4 [label=1]
	4 -> 2 [label=0]
	4 -> 3 [label=e]
	4 -> 4 [label=1]
	2 -> 2 [label=0]
	2 -> 3 [label=e]
	2 -> 4 [label=1]
}
