digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	10
	node [shape=circle]
	1 -> 2 [label=E]
	1 -> 8 [label=E]
	7 -> 10 [label=E]
	9 -> 10 [label=E]
	2 -> 3 [label=E]
	2 -> 5 [label=E]
	4 -> 7 [label=E]
	6 -> 7 [label=E]
	3 -> 4 [label=a]
	5 -> 6 [label=b]
	8 -> 9 [label=c]
}
