digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	2
	node [shape=circle]
	1 -> 2 [label=b]
	1 -> 3 [label=a]
	3 -> 4 [label=1]
	3 -> 5 [label=b]
	3 -> 6 [label=a]
	6 -> 4 [label=1]
	6 -> 5 [label=b]
	6 -> 6 [label=a]
	5 -> 4 [label=1]
	5 -> 5 [label=b]
	5 -> 6 [label=a]
	4 -> 4 [label=1]
	4 -> 5 [label=b]
	4 -> 6 [label=a]
	2 -> 4 [label=1]
	2 -> 5 [label=b]
	2 -> 6 [label=a]
}
