digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	2
	4
	5
	6
	4
	5
	6
	4
	5
	6
	4
	5
	6
	node [shape=circle]
	1 -> 2 [label=e]
	2 -> 4 [label=e]
	2 -> 5 [label=a]
	2 -> 6 [label=b]
	4 -> 4 [label=e]
	4 -> 5 [label=a]
	4 -> 6 [label=b]
	5 -> 4 [label=e]
	5 -> 5 [label=a]
	5 -> 6 [label=b]
	6 -> 4 [label=e]
	6 -> 5 [label=a]
	6 -> 6 [label=b]
}
